    <property name="ChannelID">
      <list>
        <value>FSC</value>
        <value>SSC</value>
        <value>VL1</value>
        <value>VL3</value>
        <value>YL1</value>
        <value>VL5</value>
        <value>YL3</value>
        <value>BL4</value>
        <value>VL6</value>
