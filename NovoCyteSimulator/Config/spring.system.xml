        <value>BL1</value>
        <value>VL4</value>
        <value>YL2</value>
        <value>BL3</value>
      </list>
    </property>
  </object>
  <!--488/561/640 system-->
  <object name="channelID3" type="NovoCyteSimulator.Equipment.FLChannel, NovoCyteSimulator">
    <property name="ChannelID">
      <list>
        <value>FSC</value>
        <value>SSC</value>
        <value>BL2</value>
        <value>YL1</value>
        <value>BL3</value>
        <value>YL2</value>
        <value>BL6</value>
        <value>YL5</value>
        <value>RL3</value>
        <value>YL4</value>
        <value>RL2</value>
        <value>BL1</value>
        <value>BL4</value>
        <value>YL3</value>
        <value>RL1</value>
      </list>
    </property>
  </object> 
  
</objects>